FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
convertToMeters 1;

xMin -5;
xMax 10;
yMin -1.5;
yMax 1.5;
zMin -1.5;
zMax 1.5;

vertices
(
    ($xMin $yMin $zMin)
    ($xMax $yMin $zMin)
    ($xMax $yMax $zMin)
    ($xMin $yMax $zMin)
    ($xMin $yMin $zMax)
    ($xMax $yMin $zMax)
    ($xMax $yMax $zMax)
    ($xMin $yMax $zMax)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) movingMesh (36 20 20) simpleGrading (1 1 1)

);

edges
(
);

boundary
(
    overset
    {
        type    overset;
        faces
        (
        );
    }
 
);

