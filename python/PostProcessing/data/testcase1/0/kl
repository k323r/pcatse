FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      kl;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-6;

boundaryField
{
    inlet
    {
        type            fixedValue;
		value		    uniform 1e-6;
    }

    outlet
    {
        type            zeroGradient;
    }

    wing
	{
        type            fixedValue;
        value           uniform 1e-12;
    }
    
    alula
	{
        type            fixedValue;
        value           uniform 1e-12;
    }

	".*"
	{
		type			slip;
	}
	
}


